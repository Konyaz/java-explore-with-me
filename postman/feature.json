    {
      "name": "8 - User addresses moderation comments",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "const target = pm.response.json();",
              "",
              "pm.test(\"Should return 200 when resubmitting event for moderation\", function () {",
              "    pm.response.to.have.status(200);",
              "    pm.response.to.be.withBody;",
              "    pm.response.to.be.json;",
              "});",
              "",
              "pm.test(\"Response should preserve previous moderation resolution\", function () {",
              "    pm.expect(target).to.have.property('moderationResolution');",
              "    pm.expect(target).to.have.property('moderationResolution').that.is.a('string').and.not.empty;",
              "});",
              "",
              "pm.test(\"Event state should transition to PENDING\", function () {",
              "    pm.expect(target.state).equal('PENDING');",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
