    {
      "name": "7 - User event request",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "const target = pm.response.json();",
              "",
              "pm.test(\"Should return 200 for user's own event request\", function () {",
              "    pm.response.to.have.status(200);",
              "    pm.response.to.be.withBody;",
              "    pm.response.to.be.json;",
              "});",
              "",
              "pm.test(\"Response should contain non-empty moderation resolution\", function () {",
              "    pm.expect(target).to.have.property('moderationResolution');",
              "    pm.expect(target).to.have.property('moderationResolution').that.is.a('string').and.not.empty;",
              "});",
              "",
              "pm.test(\"Event state should be MODERATION_FAILED\", function () {",
              "    pm.expect(target.state).equal('MODERATION_FAILED');",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
